config 'interface' 'loopback'
  option 'ifname' 'lo'
  option 'proto' 'static'
  option 'ipaddr' '127.0.0.1'
  option 'netmask' '255.0.0.0'

config 'interface' 'lan'
  option 'ifname' 'eth0'
  option 'type' 'bridge'
  option 'proto' 'static'
  option 'dns' '8.8.8.8'
  option 'peerdns' '0'
  option 'ipaddr' '192.168.138.129'
  option 'netmask' '255.255.255.0'
